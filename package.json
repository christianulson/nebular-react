    "@mantine/form": "^8",
    "@mantine/hooks": "^8.2.5",
    "@mantine/utils": "^6.0.22",
    "clsx": "2.1.1",
    "csstype": "3.1.3",
    "eslint-config-next": "13",
    "eva-icons": "^1.1.3",
    "html-react-parser": "5.2.6",
    "klona": "^2.0.6",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
